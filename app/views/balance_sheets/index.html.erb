<h1>Listing balance_sheets</h1>

<table>
  <tr>
    <th>Period ended</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @balance_sheets.each do |balance_sheet| %>
  <tr>
    <td><%= balance_sheet.period_ended %></td>
    <td><%= link_to 'Show', balance_sheet %></td>
    <td><%= link_to 'Edit', edit_balance_sheet_path(balance_sheet) %></td>
    <td><%= link_to 'Destroy', balance_sheet, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

